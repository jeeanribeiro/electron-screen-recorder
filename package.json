{
  "name": "electron-screen-recorder",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Simple Electron screen recorder.",
  "author": "jeeanribeiro <contact@jeanribeiro.dev>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && PROD=true vite build && electron-builder"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@quasar/extras": "^1.15.6",
    "fluent-ffmpeg": "^2.1.2",
    "quasar": "^2.10.2"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.2.3",
    "@vitejs/plugin-vue": "^3.1.2",
    "autoprefixer": "^10.4.13",
    "electron": "^21.1.0",
    "electron-builder": "^23.3.3",
    "postcss": "^8.4.19",
    "sass": "1.32.12",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vite-plugin-electron": "^0.10.4",
    "vite-plugin-electron-renderer": "^0.11.3",
    "vue": "^3.2.40",
    "vue-tsc": "^1.0.9"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344"
    }
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue",
    "screen-recorder",
    "desktop-capturer"
  ]
}
